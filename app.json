{
  "name": "V2Ray",
  "description": "Deploy V2ray on Heroku.",
  "keywords": [
    "V2Ray"
  ],
  "env": {
    "PROTOCOL": {
      "description": "协议VMess或VLESS，请填小写的vmess或vless",
      "value": "vless",
      "required": true
    },
    "UUID": {
      "description": "用户的主ID",
      "value": "",
      "required": false
    },
    "WS_PATH": {
      "description": "websocket路径",
      "value": "/ray",
      "required": true
    }
  },
  "website": "https://github.com/用户名/仓库名",
  "repository": "https://github.com/用户名/仓库名",
  "stack": "container",
  "features": [
    "runtime-dyno-metadata"
  ]
}
